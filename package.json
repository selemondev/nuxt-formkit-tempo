{
	"name": "nuxt-formkit-tempo",
	"version": "1.1.0",
	"description": "The easiest way to work with dates in Nuxt 3. The unofficial Nuxt module port of @formkit/tempo ðŸ•‘.",
	"repository": "https://github.com/selemondev/nuxt-formkit-tempo",
	"author": {
		"name": "Selemon Brahanu"
	},
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types.d.mts",
			"import": "./dist/module.mjs"
		}
	},
	"main": "./dist/module.mjs",
	"files": [
		"dist"
	],
	"keywords": [
		"nuxt3",
		"dates",
		"javascript-dates",
		"time",
		"nuxt-module"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"dev": "nuxi dev playground",
		"dev:build": "nuxi build playground",
		"dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
		"release": "npm run lint && npm run test && npm run prepack && pnpx changelogen@latest --release && npm publish && git push --follow-tags",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
	},
	"dependencies": {
		"@formkit/tempo": "^0.1.2",
		"@nuxt/kit": "^4.2.1"
	},
	"devDependencies": {
		"@nuxt/devtools": "^2.7.0",
		"@nuxt/eslint-config": "^1.10.0",
		"@nuxt/module-builder": "^1.0.2",
		"@nuxt/schema": "^4.2.1",
		"@nuxt/test-utils": "^3.20.1",
		"@types/node": "latest",
		"eslint": "^9.39.1",
		"nuxt": "^4.2.1",
		"typescript": "~5.9.3",
		"vitest": "^3.2.4",
		"vue-tsc": "^3.1.4"
	},
	"packageManager": "pnpm@9.15.9"
}
